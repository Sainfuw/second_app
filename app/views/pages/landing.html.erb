<h1 class="mb-3">Landing</h1>
<div class="row">
  <div class="col-md-11 mx-auto bg-dark">
    <table class="table table-hover">
      <thead>
        <tr>
          <th>Name</th>
          <th>Email</th>
        </tr>
      </thead>
      <tbody>
        <% @students.each do |student| %>
          <tr>
            <td><%= student.name %></td>
            <td><%= student.email %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
